{
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "pnpm typecheck && cross-env NODE_ENV=production react-router build",
    "start": "cross-env NODE_ENV=production PORT=3000 react-router-serve ./build/server/index.js",

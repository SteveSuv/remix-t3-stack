    "@types/node": "^24",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "daisyui": "^5.0.43",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prisma": "^6.11.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]

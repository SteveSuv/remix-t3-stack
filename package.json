    "@types/node": "^24",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "daisyui": "^5.1.13",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.16.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4"

{
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "pnpm typecheck && cross-env NODE_ENV=production react-router build",
    "start": "cross-env NODE_ENV=production PORT=3000 react-router-serve ./build/server/index.js",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "fmt": "prettier -w .",
    "typecheck": "react-router typegen && tsc",
    "clean": "rm -rf build node_modules pnpm-lock.yaml",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "6.4.0",
    "@react-router/fs-routes": "^7.2.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@tanstack/react-query": "^5.66.7",
    "@tanstack/react-query-devtools": "^5.66.7",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "isbot": "^5.1.22",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "react-router": "^7.2.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.7",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "daisyui": "^5.0.0-beta.6",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.3.1",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }

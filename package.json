    "@react-router/serve": "^7.9.1",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@trpc/tanstack-react-query": "^11.5.1",
    "cookie": "^1.0.2",
    "cross-env": "^10.0.0",
    "isbot": "^5.1.30",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",

    "@react-router/serve": "^7.6.3",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "isbot": "^5.1.28",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-hot-toast": "^2.5.2",
    "react-router": "^7.6.3",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.73"
